{
    "name": "lizards-and-pumpkins/lib-key-value-store-filesystem",
    "description": "Lizards & Pumpkins Filesystem KeyValueStore Library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Lizards & Pumpkins UG",
            "email": "hello@lizardsandpumpkins.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "lizards-and-pumpkins/catalog": "^0.12.0-alpha"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "lizards-and-pumpkins/coding-standards": "dev-master"
    },
    "autoload" : {
        "psr-4": {
            "LizardsAndPumpkins\\DataPool\\KeyValueStore\\File\\": "src/"
        }
    },
    "scripts" : {
        "test": "vendor/bin/phpunit -c tests/Unit/phpunit.xml.dist",
        "sniff" : "vendor/bin/phpcs -p --colors --standard=vendor/lizards-and-pumpkins/coding-standards/src/LizardsAndPumpkins/ src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
