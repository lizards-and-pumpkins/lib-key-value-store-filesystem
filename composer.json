{
    "name": "brera/lib-key-value-store",
    "description": "Brera KeyValueStore Library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vinai Kopp",
            "email": "vinai@brera.io"
        },
        {
            "name": "Tim Bezhashvyly",
            "email": "tim@brera.io"
        },
        {
            "name": "Fabian Blechsmidt",
            "email": "fabian@brera.io"
        }
    ],
    "require": {
        "brera/proof-of-concept": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "brera/coding-standards": "dev-master"
    },
    "autoload" : {
        "psr-4": {
            "Brera\\DataPool\\KeyValue\\InMemory\\": "src/",
            "Brera\\DataPool\\KeyValue\\File\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.brera.io"
        }
    ],
    "scripts" : {
        "sniff" : "vendor/bin/phpcs -p --colors --standard=vendor/brera/coding-standards/src/Brera/ src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
