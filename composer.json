{
    "name": "lizards-and-pumpkins/lib-key-value-store",
    "description": "Lizards & Pumpkins KeyValueStore Library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vinai Kopp",
            "email": "vinai@lizardsandpumpkins.com"
        },
        {
            "name": "Tim Bezhashvyly",
            "email": "tim@lizardsandpumpkins.com"
        },
        {
            "name": "Fabian Blechschmidt",
            "email": "fabian@lizardsandpumpkins.com"
        }
    ],
    "require": {
        "php": "^5.6.0|^7.0",
        "lizards-and-pumpkins/catalog": "0.2.0-alpha"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "lizards-and-pumpkins/coding-standards": "dev-master"
    },
    "autoload" : {
        "psr-4": {
            "LizardsAndPumpkins\\DataPool\\KeyValue\\InMemory\\": "src/",
            "LizardsAndPumpkins\\DataPool\\KeyValue\\File\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.lizardsandpumpkins.com"
        }
    ],
    "scripts" : {
        "sniff" : "vendor/bin/phpcs -p --colors --standard=vendor/lizards-and-pumpkins/coding-standards/src/LizardsAndPumpkins/ src/ tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
